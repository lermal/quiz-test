<IfModule mod_headers.c>
	<FilesMatch "\.(woff2?|ttf|eot)$">
		Header set Cache-Control "public, max-age=31536000, immutable"
	</FilesMatch>
	<FilesMatch "\.(png|jpe?g|gif|webp|avif|ico|svg)$">
		Header set Cache-Control "public, max-age=31536000, immutable"
	</FilesMatch>
	<FilesMatch "\.(css|js)$">
		Header set Cache-Control "public, max-age=31536000, immutable"
	</FilesMatch>
</IfModule>
